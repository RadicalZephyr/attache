#!/usr/bin/env bash

source ../.environment

curl https://raw.githubusercontent.com/RadicalZephyr/attache/master/install.sh | sh

RET=$?

if [ $RET -ne 0 ]
then
    echo "Install script exited with status: $RET."
    exit 1
fi

if [ \! -d $INSTALL_DIR ]
then
    echo "Installed attache not found at $INSTALL_DIR."
    exit 1
fi

exit 0
